    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.12.1",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "tape": "^4.8.0",
    "webpack": "^3.8.1"
  }
