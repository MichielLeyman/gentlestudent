  "bugs": {
    "url": "https://github.com/drdynscript/gentlestudent-app/issues"
  },
  "homepage": "https://github.com/drdynscript/gentlestudent-app#readme",
  "keywords": [
    "gentlestudent",
    "node",
    "heroku",
    "express"
  ],
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "joi": "^13.0.2",
    "mongoose": "^4.13.4",
    "morgan": "^1.9.0",
