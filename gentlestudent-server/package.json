{
  "name": "gentlestudent-app",
  "version": "1.0.0",
  "description": "Gentlestudent. Wijk en studenten vinden elkaar in duurzame initiatieven.",
  "engines": {
    "node": "8.9.1"
  },
  "main": "server.js",
  "scripts": {
    "build:server": "babel server.js server/* --out-dir build",
    "heroku-prebuild": "echo \"This runs before Heroku installs the dependencies.\"",
    "heroku-postbuild": "echo \"This runs afterwards.\"",
    "start": "NODE_ENV=development nodemon server.js --exec babel-node",
    "start:dev": "NODE_ENV=development PORT=8080 nodemon server.js --exec babel-node",
    "start:build": "npm run build:server && NODE_ENV=production PORT=8080 node build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drdynscript/gentlestudent-app.git"
  },
